{
  "compilerOptions": {
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,

    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "jsx": "react",

    "strict": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "pretty": true
  }
}
